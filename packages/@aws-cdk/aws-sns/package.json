{
  "name": "@aws-cdk/aws-sns",
  "version": "0.0.0-PLACEHOLDER",
  "description": "CDK Constructs for AWS SNS",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "software.amazon.awscdk.services.sns",
        "maven": {
          "groupId": "software.amazon.awscdk",
          "artifactId": "sns"
        }
      },
      "dotnet": {
        "namespace": "Amazon.CDK.AWS.SNS",
        "packageId": "Amazon.CDK.AWS.SNS",
        "signAssembly": true,
        "assemblyOriginatorKeyFile": "../../key.snk"
      },
      "python": {
        "distName": "aws-cdk.aws-sns",
        "module": "aws_cdk.aws_sns"
      }
    },
    "excludeTypescript": [
      "examples"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/awslabs/aws-cdk.git",
    "directory": "packages/@aws-cdk/aws-sns"
  },
  "cdk-build": {
    "cloudformation": "AWS::SNS"
  },
  "keywords": [
    "aws",
    "cdk",
    "constructs",
    "sns"
  ],
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com",
    "organization": true
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-cdk/aws-autoscaling-api": "0.0.0-PLACEHOLDER",
    "@aws-cdk/aws-cloudwatch": "0.0.0-PLACEHOLDER",
    "@aws-cdk/aws-events": "0.0.0-PLACEHOLDER",
    "@aws-cdk/aws-iam": "0.0.0-PLACEHOLDER",
    "@aws-cdk/aws-lambda": "0.0.0-PLACEHOLDER",
    "@aws-cdk/aws-s3-notifications": "0.0.0-PLACEHOLDER",
    "@aws-cdk/aws-sqs": "0.0.0-PLACEHOLDER",
    "@aws-cdk/cdk": "0.0.0-PLACEHOLDER"
  },
  "homepage": "https://github.com/awslabs/aws-cdk",
  "engines": {
    "node": ">= 8.10.0"
  },
  "awslint": {
    "exclude": [
      "construct-base-is-private:@aws-cdk/aws-sns.TopicBase"
    ]
  }
}
